<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

	<bean id="eircodeService" class="br.com.fexco.address.service.EircodeService"></bean>
		<bean id="addressService" class="br.com.fexco.address.service.AddressService">
		 <property name="ericodeService" ref="eircodeService" />
	</bean>
	
	<bean id="addressEircodeController" class="br.com.fexco.address.controller.AddressEircodeController">
		 <property name="addressService" ref="addressService" />
	</bean>
	

</beans>
